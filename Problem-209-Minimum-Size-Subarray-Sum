from typing import List
class Solution:
    def minSubArrayLen(self, target: int, nums: List[int]) -> int:
        current_sum=0
        left=0
        mini=float("inf")
        for right in range(len(nums)):
            current_sum+=nums[right]
            while current_sum>=target:
                mini=min(mini,right-left+1)
                current_sum-=nums[left]
                left+=1
        if mini==float("inf"):
            return 0
        return mini

