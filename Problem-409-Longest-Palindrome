class Solution:
    def longestPalindrome(self, s: str) -> int:
        d={}
        for i in s:
            if i not in d:
                d[i]=1
            else:
                d[i]+=1
        res=0
        for key,value in d.items():
            if value%2==0:
                res+=value
            else:
                res+=(value-1)
                d[key]=1
        if 1 in d.values():
            res+=1
        return res