from typing import List
class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        prefix=strs[0]
        prefixlen=len(prefix)
        for i in strs[1:]:
            while prefix!=i[:prefixlen]:
                prefixlen-=1
                if prefixlen==0:
                    return  ""
                prefix=prefix[:prefixlen]
        return prefix

